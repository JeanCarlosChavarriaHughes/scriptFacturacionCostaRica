            "libraries/phpmailer/class.phpmaileroauth.php",
            "libraries/phpmailer/class.phpmaileroauthgoogle.php",
            "libraries/phpmailer/lass.smtp.php",
            "libraries/phpmailer/lass.pop3.php",
            "libraries/phpmailer/xtras/EasyPeasyICS.php",
            "libraries/phpmailer/xtras/ntlm_sasl_client.php"
